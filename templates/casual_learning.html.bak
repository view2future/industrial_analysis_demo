    <script>
        // Region selection functionality
        document.addEventListener('DOMContentLoaded', function() {
            // Initially show only Chengdu and hide all others
            document.querySelectorAll('.region-card').forEach(card => {
                card.style.display = 'none';  // Ensure all are hidden initially
            });
            document.getElementById('region-chengdu').style.display = 'block';  // Show Chengdu
            document.getElementById('region-chengdu').classList.add('active');  // Add active class

            // Load leadership data for the initially displayed region
            loadLeadershipData('chengdu');

            // Add click listeners to region selector buttons
            const regionButtons = document.querySelectorAll('.sidebar-nav-link');
            regionButtons.forEach(button => {
                button.addEventListener('click', function(e) {
                    e.preventDefault();
                    const region = this.dataset.region;

                    // Update active state for buttons
                    regionButtons.forEach(btn => btn.classList.remove('active'));
                    this.classList.add('active');

                    // Show selected region and hide others
                    document.querySelectorAll('.region-card').forEach(card => {
                        if (card.id === `region-${region}`) {
                            card.style.display = 'block';
                            // Load leadership data for the selected region
                            loadLeadershipData(region);
                        } else {
                            card.style.display = 'none';
                        }
                    });
                });
            });
        });

